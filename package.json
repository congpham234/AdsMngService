{
  "name": "deliveryservice",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prebuild": "npm run lint && npm run coverage && rm -rf dist",
    "build": "esbuild src/lambda-handler.ts --bundle --minify --sourcemap --platform=node --target=es2020 --outfile=dist/index.js",
    "postbuild": "cd dist && zip -r index.zip index.js*",
    "test": "jest --detectOpenHandles",
    "coverage": "jest --coverage",
    "lint": "eslint .",
    "format": "npm run lint -- --fix"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-serverless-express": "^3.4.0",
    "express": "^4.18.2",
    "husky": "^9.0.6"
  },
  "devDependencies": {
    "@types/aws-lambda": "8.10.131",
    "@types/aws-serverless-express": "^3.3.9",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.6",
    "@types/supertest": "^6.0.2",
    "aws-sdk-mock": "^5.9.0",
    "esbuild": "0.19.11",
    "eslint": "^8.56.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-typescript": "^3.0.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build",
      "pre-push": "npm run build"
    }
  }
}
